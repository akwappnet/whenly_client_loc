{
  "name": "Whenly",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android_clean": "cd android && ./gradlew clean && cd ..",
    "android:internal": "cd android && fastlane internal",
    "android:beta": "cd android && fastlane beta",
    "android:release": "cd android && fastlane deploy",
    "ios": "react-native run-ios",
    "ios:tf": "cd ios && fastlane testFlight",
    "ios:release": "cd ios && fastlane release",
    "start": "react-native start",
    "start:development": "APP_ENV=development react-native start",
    "android_development": "APP_ENV=development react-native run-android",
    "build_android_development": "export APP_ENV=development && cd android && ./gradlew assembleRelease && cd ..",
    "start:staging": "APP_ENV=staging react-native start",
    "start:prod": "APP_ENV=production react-native start",
    "certs": "cd ios/ && fastlane certificates",
    "clean:all": "rm -rf /tmp/metro-* && rm -rf node_modules && rm -rf package-lock.json && rm -rf yarn.lock && cd ios/ && rm -rf Pods/ && rm -rf Podfile.lock",
    "clean": "rm -rf /tmp/metro-* && rm -rf node_modules && cd ios/ && rm -rf Pods/",
    "reinstall": "yarn install && cd ios/ && pod install --repo-update",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "patch-package",
    "build_android_production_bundle": "export SET NODE_OPTIONS=--openssl-legacy-provider && export APP_ENV=.env.production && cd android && ./gradlew bundleRelease && cd .."
  },
